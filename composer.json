{
    "name": "sprigs/expandable-row",
    "description": "A Laravel Nova packgage that moves the preview from the modal to an expandable row.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0"
    },
    "autoload": {
        "psr-4": {
            "SPRIGS\\ExpandableRow\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SPRIGS\\ExpandableRow\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
